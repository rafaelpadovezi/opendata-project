curl -o datafiles/worldbank/gc.dod.totl.gd.zs.zip http://api.worldbank.org/v2/en/indicator/gc.dod.totl.gd.zs?downloadformat=csv
unzip datafiles/worldbank/gc.dod.totl.gd.zs.zip -d datafiles/worldbank/gc.dod.totl.gd.zs
rm datafiles/worldbank/gc.dod.totl.gd.zs.zip
rm datafiles/worldbank/gc.dod.totl.gd.zs/[Content_Types].xml

